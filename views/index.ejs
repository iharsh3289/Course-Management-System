          <div class="form-group">
            <label for="recipient-name" class="col-form-label">Email id</label>
            <input type="email" class="form-control" placeholder="Email id" name="email" id="recipient-name4" required="">
          </div>
       <!--   <div class="form-group">
            <label for="recipient-name" class="col-form-label">Mobile Number</label>
            <input type="text" class="form-control" placeholder="Mobile Number" name="Name" id="recipient-name5" required="">
          </div>
        -->
          <div class="form-group">
            <label for="recipient-name1" class="col-form-label">Password</label>
            <input type="password" class="form-control" placeholder="Password" name="psw" id="recipient-name6" required="">
          </div>
          <div class="form-group">
            <label for="recipient-name1" class="col-form-label">Repeat Password</label>
            <input type="password" class="form-control" placeholder="Password" name="psw_repeat" id="recipient-name6" required="">
          </div>
          <div class="form-group">
            <label for="recipient-name1" class="col-form-label">I am a</label>
            <select class="form-control" name="role">
              <option value="Student">Student</option>
              <option value="Instructor">Instructor</option>
            </select>
          </div>
          <div class="right-w3l mt-4 mb-3">
            <input type="submit" class="form-control" value="Create account">
          </div>
        </form>

      </div>
    </div>
  </div>
</div>
